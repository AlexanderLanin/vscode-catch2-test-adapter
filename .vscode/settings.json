{
  "files.exclude": {
    ".git": true,
    "node_modules": true,
    "out": true
  },
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "[json]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "files.associations": {
    "catch.hpp.txt": "cpp"
  },
  "editor.formatOnSaveTimeout": 3000,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    {
      "language": "typescript",
      "autoFix": true
    }
  ],
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false,
  },
  "[typescript]": {
    "editor.formatOnSave": false,
  },
  "prettier.eslintIntegration": true
}