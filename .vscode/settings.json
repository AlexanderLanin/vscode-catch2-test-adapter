{
  "files.exclude": {
    ".git": true,
    "node_modules": true,
    "out": true
  },
  "editor.insertSpaces": false,
  "editor.tabSize": 2,
  "[json]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "prettier.printWidth": 100,
  "prettier.jsxBracketSameLine": true,
  "editor.formatOnSave": true,
  "files.associations": {
    "catch.hpp.txt": "cpp"
  },
  "editor.formatOnSaveTimeout": 3000
}
