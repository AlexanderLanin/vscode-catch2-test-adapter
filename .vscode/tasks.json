{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "build",
      "problemMatcher": [
        {
          "owner": "ts",
          "source": "typescript",
          "fileLocation": "relative",
          "pattern": [
            {
              "regexp": "^([^:]+):([0-9]+):([0-9]+) - (error) (.*)$",
              "file": 1,
              "line": 2,
              "column": 3,
              "severity": 4,
              "message": 5
            }
          ]
        }
      ]
    }
  ]
}
